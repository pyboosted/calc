{
  "name": "boosted-calc",
  "version": "1.1.6",
  "description": "A powerful terminal-based calculator with unit conversions, currency support, and more",
  "keywords": [
    "calculator",
    "cli",
    "terminal",
    "unit-conversion",
    "currency",
    "math",
    "typescript",
    "bun"
  ],
  "author": "pyboosted",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pyboosted/calc.git"
  },
  "homepage": "https://github.com/pyboosted/calc#readme",
  "bugs": {
    "url": "https://github.com/pyboosted/calc/issues"
  },
  "module": "src/index.tsx",
  "type": "module",
  "bin": {
    "calc": "bin/calc"
  },
  "files": [
    "src/**/*",
    "bin/**/*",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "bun run src/cli.tsx",
    "dev": "bun run --watch src/cli.tsx",
    "update-currencies": "bun run src/cli.tsx --update",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19.1.8"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "clipboardy": "^4.0.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "ink": "^6.0.0",
    "react": "^19.1.0",
    "yaml": "^2.8.0"
  }
}